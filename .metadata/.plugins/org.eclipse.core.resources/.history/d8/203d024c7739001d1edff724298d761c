class Employee {
	String name;
	int salary;
	
	public void getEmployee() {
		System.out.println(name + salary);
	}
	
}

class Manager extends Employee{
	String depart;
	
	public void getManager() {
		getEmployee();
		System.out.println();
	}
	
}